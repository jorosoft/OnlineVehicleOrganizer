@using OVO.Web.Areas.Administration.ViewModels
@model UsersListViewModel


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h1>Users</h1>
    @foreach (var user in Model.Users)
    {
        <p>@user.Username --- @user.Email --- @user.IsDeleted</p>
    }
    
</div> 

